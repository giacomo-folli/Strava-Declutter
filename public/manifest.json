{
  "manifest_version": 3,
  "name": "Strava Declutter",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["*://*.strava.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
}